{
	"extends": ["onepass"],
	"rules": {
		"no-console": 0,
		"class-methods-use-this": 0,
		"prettier/prettier": [
			"error",
			{
				"trailingComma": "es5",
				"singleQuote": true,
				"printWidth": 120,
				"tabWidth": 4
			}
		]
	},
	"settings": {
		"import/resolver": {
			"node": {
				"extensions": [".js", ".ts"],
				"moduleDirectory": ["node_modules", "src/"]
			}
		}
	}
}
